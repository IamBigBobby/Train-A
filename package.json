{
  "name": "train-a",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "eslint --fix \"src/**/*.ts\" && stylelint --fix \"src/**/*.scss\"",
    "stylelint": "stylelint src/**/*.scss",
    "format": "prettier --write \"src/**/*.{ts,scss,html}\" && eslint --fix \"src/**/*.ts\" && stylelint --fix \"src/**/*.scss\"",
    "clean": "rm -rf dist && rm -rf node_modules"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.0",
    "@angular/cdk": "18.0.0",
    "@angular/common": "18.2.0",
    "@angular/compiler": "18.2.0",
    "@angular/core": "18.2.0",
    "@angular/forms": "18.2.0",
    "@angular/platform-browser": "18.2.0",
    "@angular/platform-browser-dynamic": "18.2.0",
    "@angular/router": "18.2.0",
    "@ngrx/effects": "18.0.2",
    "@ngrx/store": "18.0.2",
    "@ngrx/store-devtools": "18.0.2",
    "@planess/train-a-backend": "^0.0.6",
    "@taiga-ui/addon-charts": "4.2.0",
    "@taiga-ui/addon-commerce": "4.2.0",
    "@taiga-ui/addon-doc": "4.2.0",
    "@taiga-ui/addon-mobile": "4.2.0",
    "@taiga-ui/addon-table": "4.2.0",
    "@taiga-ui/cdk": "4.2.0",
    "@taiga-ui/core": "4.2.0",
    "@taiga-ui/event-plugins": "4.0.2",
    "@taiga-ui/icons": "4.2.0",
    "@taiga-ui/kit": "4.2.0",
    "@taiga-ui/layout": "4.2.0",
    "@taiga-ui/legacy": "^4.3.0",
    "@taiga-ui/styles": "4.2.0",
    "leaflet": "^1.9.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.0",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular/cli": "18.2.0",
    "@angular/compiler-cli": "18.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.12",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "angular-eslint": "18.3.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.1.3",
    "husky": "9.1.4",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "15.2.9",
    "prettier": "3.3.3",
    "stylelint": "16.8.2",
    "stylelint-config-clean-order": "6.1.0",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard": "36.0.1",
    "stylelint-config-standard-scss": "13.1.0",
    "stylelint-scss": "6.5.0",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.1.0"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ],
    "**/*.{scss,css}": [
      "prettier --write",
      "stylelint --fix"
    ],
    "**/*.html": [
      "prettier --write"
    ]
  }
}
